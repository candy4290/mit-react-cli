{
  "name": "demoName",
  "author": {
    "name": "demoAuthor"
  },
  "version": "demoVersion",
  "private": true,
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@craco/craco": "^7.0.0",
    "@loadable/component": "^5.15.2",
    "@reduxjs/toolkit": "^1.8.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "ahooks": "^3.7.0",
    "antd": "^5.1.2",
    "axios": "^1.1.3",
    "core-js": "^3.17.3",
    "craco-less": "^2.1.0-alpha.0",
    "css-loader": "^6.7.1",
    "dotenv-cli": "^4.0.0",
    "http-proxy-middleware": "^1.0.5",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "qs": "^6.9.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.4.4",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0"
  },
  "homepage": "/",
  "scripts": {
    "reinstall": "rd /s/ q node_modules && npm install",
    "start": "dotenv -e .env craco start",
    "start:prod": "dotenv -e .env.production craco start",
    "build": "dotenv -e .env craco build",
    "build:prod": "dotenv -e .env.production craco build",
    "test": "craco test",
    "eject": "npx react-scripts eject",
    "serve": "serve -s build",
    "analyz": "webpack-bundle-analyzer --port 8888 ./build/stats.json",
    "compressImg": "cd ./scripts/imgs && node compress.js",
    "compressImgSingle": "cd ./scripts/imgs && node compress-single.js",
    "checkLargeImg": "cd ./scripts/imgs && node check-big-img.js",
    "version": "cd ./scripts/version && node index.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.2.1-rc.0-next-4de99b3ca-20220221",
    "husky": "^4.3.0",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "simple-progress-webpack-plugin": "^2.0.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-resources-loader": "^1.5.0",
    "webpack-bundle-analyzer": "^4.4.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./scripts/git/commit-msg.js -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint"
    ]
  }
}
